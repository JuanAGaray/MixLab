{
    "builds": [
      {
        "src": "frozz/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "30mb",
          "runtime": "python3.12"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frozz/wsgi.py"
      }
    ]
  }